{
  "name": "jlchassaing/facetbundle",
  "description": "GieFacetBundle helps is a set of helpers to create search Facets, and perform sorl queries with those facets",
  "type": "ezplatform-bundle",
  "keywords": [
    "ezplatform",
    "ezpublish",
    "facets",
    "tags"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jean-Luc Chassaing"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ezsystems/ezplatform-kernel": "^1.1",
    "ezsystems/ezplatform-solr-search-engine": "^3.1",
    "netgen/tagsbundle": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Gie\\FacetBundle\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
